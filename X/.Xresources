! urxvt config
URxvt.scrollBar:	false
URxvt.scrollstyle:      plain
URxvt.secondaryScroll:  true
URxvt.pointerBlank:	true
URxvt.saveLines:        5000
!URxvt.visualBell:      true
URxvt.font:             xft:Hack:autohint=true:size=15, \
                        xft:Noto Sans CJK JP:autohint=true:size=18
URxvt.boldFont:         xft:Hack:bold:autohint=true:size=15
URxvt.italicFont:       xft:Hack:italic:autohint=true:size=15
URxvt.boldItalicFont:   xft:Hack:bold:italic:autohint=true:size=15
!URxvt.intensityStyles:	false

! emacs
Emacs.font: Hack-15


